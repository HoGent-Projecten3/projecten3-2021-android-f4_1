+import com.example.faithandroid.models.GoalPost
 import com.example.faithandroid.models.TextPost
 
 class SkyscraperViewModel : ViewModel() {
 
+
+    var lijst: List<String> = listOf("homos", "hello")
+
+    public var testData = mutableListOf<GoalPost>(
+        GoalPost(
+            "Sorry zeggen tegen Bob",
+            "Ik heb hem gekwetst en moet sorry zeggen",
+            false,
+            lijst
+        ),
+        GoalPost(
+            "Huiswerk maken",
+            "Huiswerk maken voor Frans",
+            false,
+            lijst
+        )
+    )
+
+    private val _testLive = MutableLiveData<List<GoalPost>>()
+
+    val testLive: LiveData<List<GoalPost>>
+            get() = _testLive
+
+
+    fun addGoalPost(goalPost: GoalPost){
+        testData.add(goalPost)
+        _testLive.value = (testData);
+    }
+
 }
